#!/bin/bash
pkgver=$(wget -cqO- http://sourceforge.net/projects/pymol | grep "\.tar\.bz2" | cut -d '"' -f 4 | cut -d ' ' -f 2 | sed 's/pymol\-v//g' | sed 's/\.tar\.bz2//g')
pkgpver=$(cat /data/OBS/home:fusion809/pymol/pymol.spec | grep "Version:" | cut -d ':' -f 2 | sed 's/\s*//g')

if [[ $pkgver == $pkgpver ]]; then
    printf "PyMOLï…¤\n"
else
    printf "PyMOL${pkgver}\n"
fi
