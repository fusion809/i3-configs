#!/bin/bash
function calc {
    TZ="Australia/Brisbane" date +"%$1"
}

S=$(calc S)
M=$(calc M)
H=$(calc H)
D=$(calc d)
m=$(calc m)
Y=$(calc Y)

OS=$(cat /etc/os-release | grep "^ID" | cut -d '=' -f 2 | head -n 1)

import -window root -frame -silent "/home/fusion809/Pictures/Screenshots/i3/Screenshot_${OS}_${Y}${m}${D}_${H}${M}${S}.png"
